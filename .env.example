# GOOGLE GEMINI API CONFIGURATION
# Get your API key from: https://aistudio.google.com/welcome

GOOGLE_API_KEY=your_gemini_api_key

# MODEL CONFIGURATION
# Track A Model: Used for global context analysis (full document understanding)
TRACK_A_MODEL=gemini-3-flash-preview

# Track B Model: Used for agentic deep search (iterative exploration)
TRACK_B_MODEL=gemini-3-flash-preview

# Consensus Model: Used for master validation and conflict resolution
CONSENSUS_MODEL=gemini-3-flash-preview

# PDF Conversion Provider: 'docling' (local, tables++, slow) or 'gemini' (cloud, fast, structured)
CONVERSION_PROVIDER=gemini


# PIPELINE CONFIGURATION
# Output directory for all generated files (markdown, JSON, etc.)
OUTPUT_DIR=output

# Enable debug logging for detailed execution traces
DEBUG_MODE=false

# Maximum retries for API calls in case of transient failures
MAX_RETRIES=3

# Timeout for API calls (in seconds)
API_TIMEOUT=120
